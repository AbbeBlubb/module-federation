{
  "name": "module-federation",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install-infrastructure": "cd packages/infrastructure && npm install",
    "install-public": "cd packages/public && npm install",
    "install-modules": "cd packages/modules && npm install",
    "c:install": "concurrently \"npm:install-*\"",
    "start-infrastructure": "cd packages/infrastructure && npm start",
    "start-public": "cd packages/public && npm start",
    "start-modules": "cd packages/modules && npm start",
    "c:start": "concurrently --kill-others \"npm:start-*\"",
    "dev-infrastructure": "cd packages/infrastructure && npm run start:live",
    "dev-public": "cd packages/public && npm run start:live",
    "dev-modules": "cd packages/modules && npm run start:live",
    "c:dev": "concurrently --kill-others \"npm:dev-*\""
  },
  "keywords": [],
  "author": "Alberto F. Chávez Pérez",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
